pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        sh 'docker build -t skander1919/dp-alpine:latest .'
      }
    }
    stage('Push image') {        
        docker.withRegistry('', 'dock-cred') {
        docker tag skander1919/dp-alpine skander1919/dp-alpine:latest
        docker push skander1919/dp-alpine:latest
        }
    }
  }
}
